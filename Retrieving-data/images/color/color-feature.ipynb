    "import cv2\n",
    "\n",
    "# Load an image\n",
    "image = cv2.imread(\"example.jpg\")\n",
    "\n",
    "# Get the RGB values of a pixel at (x=50, y=100)\n",
    "b, g, r = image[100, 50]  # OpenCV loads images in BGR format by default\n",
    "print(f\"Red: {r}, Green: {g}, Blue: {b}\")"
